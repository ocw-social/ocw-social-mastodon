+
+// Styling from upstream's WebUI, as public pages use the same layout
+.embed {
+  .status {
+    .status__info {
+      font-size: 15px;
+      display: initial;
+    }
+
+    .status__relative-time {
+      color: $dark-text-color;
+      float: right;
+      font-size: 14px;
+      width: auto;
+      margin: initial;
+      padding: initial;
+      padding-bottom: 1px;
