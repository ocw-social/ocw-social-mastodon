+
+.simple_form {
+  &.hidden {
+    display: none;
+  }
+
+  .input {
+    margin-bottom: 15px;
+    overflow: hidden;
+
+    &.hidden {
+      margin: 0;
+    }
+
+    &.radio_buttons {
